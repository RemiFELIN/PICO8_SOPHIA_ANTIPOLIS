function collide_obj(p,o,distance)
  local py = p.y  local d = distance or 0
  if player.down and p == player then py += 8 end
  return p.x<=o.x+o.w+d and
         o.x<=p.x+p.w+d and
         py<=o.y+o.h+d and
         o.y<=py+p.h+d
end


function str_to_spr(s,x,y,w,h)
for yy=0,h-1 do
  for xx=0,w-1 do
  local n=0
  local s=sub(s,xx+yy*w+1,xx+yy*w+1)
  if s~="a" then
    n=("0x"..s)+0
    pset(x+xx,y+yy,n)
  end
  end
end
end

