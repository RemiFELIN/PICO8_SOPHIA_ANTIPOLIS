function boss_animate()
  if boss.hit then
    boss.sp = 105
    boss.hit = false
    boss.anim=time()
  else
    if del_boss > 12 then
      del_boss -=1
      if time()-boss.anim>.3 then
        boss.anim=time()
        boss.sp+=2
